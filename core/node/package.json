{
  "name": "core-node",
  "version": "1.0.0",
  "description": "Core node for the DeAI network",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run proto:gen && tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "proto:gen": "grpc_tools_node_protoc --proto_path=./src/grpc --js_out=import_style=commonjs,binary:./src/grpc --grpc_out=grpc_js:./src/grpc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=service=grpc-node:./src/grpc ./src/grpc/node.proto"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "grpc-tools": "^1.12.4",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-protoc-gen": "^0.15.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.14",
    "@grpc/proto-loader": "^0.7.10",
    "ethers": "^6.15.0",
    "express": "^4.18.2"
  }
}
